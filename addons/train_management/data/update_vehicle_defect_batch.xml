<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="ir_cron_update_vehicle_defect_batch" model="ir.cron">
            <field name="name">Update Vehicle Defect Batch</field>
            <field name="model_id" ref="model_train_management_vehicle_defect_batch_update"/>
            <field name="state">code</field>
            <field name="code">model.update_vehicle_defect()</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
    </data>
</odoo>
